<!DOCTYPE html>
<html>
<head>
    <style>
        #map {
            height: 400px; 
            width: 100%; 
        }
    </style>
</head>
<body>
    <h3>Google Maps Demo</h3>

    <div id="map"></div>
    <script>
        function initMap() {
            var katowice = { lat: 50.272, lng: 19.029 };
            var map = new google.maps.Map(
                document.getElementById('map'), { zoom:16, center: katowice });
            var marker = new google.maps.Marker({ position: katowice, map: map });
        }
    </script>
<script async defer
        src="https://maps.googleapis.com/maps/api/js?key=AIzaSyADGXBkm7HNisU6_HHXmOGXhQPmL9RYlPo&callback=initMap">
</script>
</body>
</html>



